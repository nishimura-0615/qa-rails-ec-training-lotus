<h2>タスク一覧</h2>
<table>
    <thead>
      <tr>
        <th>タスク名</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.title %></td>
          <td><%= link_to "編集", edit_user_path(user) %></td>
          <td><%= link_to "削除", user_path(user), method: :delete %></td>
        </tr>
      <% end %>
    </tbody>
</table>
<%= link_to "タスク追加", new_user_path %>
<%= link_to "ログアウト", logout_path, method: :delete %>
